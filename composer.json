{
  "name": "klimser/5plus",
  "description": "5plus.uz website",
  "keywords": [
    "5plus",
    "yii2",
    "framework"
  ],
  "homepage": "https://5plus.uz/",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "support": {
    "issues": "https://5plus.uz/contacts"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-bcmath": "*",
    "ext-simplexml": "*",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-jui": "^2.0",
    "kartik-v/yii2-widget-activeform": "^1.4",
    "2amigos/yii2-tinymce-widget": "~1.1",
    "tinify/tinify": "^1.4",
    "kartik-v/yii2-field-range": "^1",
    "google/apiclient": "^2.2",
    "yiisoft/yii2-authclient": "^2.1",
    "himiklab/yii2-recaptcha-widget": "^2",
    "phpoffice/phpspreadsheet": "^1.4",
    "picqer/php-barcode-generator": "^2",
    "setasign/fpdi": "^2.2",
    "chillerlan/php-qrcode": "^4",
    "yiisoft/yii2-bootstrap4": "~2.0",
    "yiisoft/yii2-twig": "^2.4",
    "tecnickcom/tcpdf": "^6.3",
    "kartik-v/yii2-widget-datetimepicker": "^1",
    "yiisoft/yii2-redis": "^2.0",
    "phpoffice/phpword": "^1",
    "longman/telegram-bot": "*"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
